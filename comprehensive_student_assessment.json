          "required": true,
          "weight": 2,
          "graphable": true,
          "options": [
            {
              "id": "excellent-focus",
              "label": "Excellent - Sustained attention across activities",
              "value": 1,
              "children": [
                {
                  "id": "focus-duration",
                  "type": "question",
                  "title": "Average duration of sustained focus (minutes)",
