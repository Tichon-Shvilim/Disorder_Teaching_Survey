                  "weight": 1,
                  "graphable": true,
                  "options": [
                    { "id": "personal-interest", "label": "Personal Interest in Topic", "value": 1 },
                    { "id": "interactive-methods", "label": "Interactive Teaching Methods", "value": 2 },
                    { "id": "clear-structure", "label": "Clear Structure and Expectations", "value": 3 },
                    { "id": "frequent-breaks", "label": "Frequent Breaks", "value": 4 }
                  ]
                }
              ]
            },
            {
              "id": "attention-challenges",
              "label": "Significant attention challenges",
              "value": 3,
              "children": [
                {
                  "id": "attention-symptoms",
                  "type": "question",
                  "title": "Which attention-related symptoms are most prominent?",
                  "inputType": "multiple-choice",
                  "required": true,
                  "weight": 2,
                  "graphable": true,
                  "options": [
                    { "id": "distractibility", "label": "High Distractibility", "value": 1 },
                    { "id": "impulsivity", "label": "Impulsive Behaviors", "value": 2 },
                    { "id": "hyperactivity", "label": "Hyperactivity/Restlessness", "value": 3 },
                    { "id": "task-avoidance", "label": "Task Avoidance", "value": 4 }
                  ]
                },
                {
                  "id": "accommodation-effectiveness",
                  "type": "question",
                  "title": "Rate the effectiveness of current accommodations",
                  "inputType": "scale",
                  "scaleMin": 1,
                  "scaleMax": 5,
                  "required": true,
                  "weight": 1,
                  "graphable": true
                }
              ]
            }
          ]
        }
      ]
