                  "required": true,
                  "weight": 1,
                  "graphable": true,
                  "options": [
                    { "id": "no-concerns", "label": "No concerns", "value": 1 },
                    { "id": "minor-transition", "label": "Minor transition difficulties", "value": 2 },
                    { "id": "significant-hyperfocus", "label": "Significant hyperfocus issues", "value": 3 }
