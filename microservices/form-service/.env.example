# Form Service Environment Variables
# Copy to .env in production and fill with actual values

# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/formDB?retryWrites=true&w=majority

# Security
JWT_SECRET=your-super-secure-jwt-secret-here

# Server
PORT=3003
NODE_ENV=production

# CORS
CORS_ORIGIN=https://your-frontend.onrender.com

# Service URLs
USER_SERVICE_URL=https://your-user-service.onrender.com
STUDENT_SERVICE_URL=https://your-student-service.onrender.com
